CREATE TABLE CategoriesEntity (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    icon TEXT NOT NULL
);

getAllCategories:
SELECT *
FROM CategoriesEntity;

insertCategory:
INSERT OR REPLACE
INTO CategoriesEntity
VALUES (?,?,?);

deleteCategoryById:
DELETE
FROM CategoriesEntity
WHERE id = :id;

replaceCategory:
UPDATE CategoriesEntity SET name = :name, icon = :icon
WHERE id = :id;